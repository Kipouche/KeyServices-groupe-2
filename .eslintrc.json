{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "class-methods-use-this": "off",
        "react/prop-types": 0,
        "react/jsx-filename-extension": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-props-no-spreading": "off"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    }
}